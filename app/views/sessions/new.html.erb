<h1> Login </h1>

<h4><%= flash[:notice] %> </h4>

<%= form_tag login_path, method: :post do %>
    <%= label_tag "Username" %>
    <%= text_field_tag :username %>
    <%= label_tag "Password" %>
    <%= text_field_tag :password %>
    <%= submit_tag "Login" %>
<% end %>
<br>
<%= link_to "Create Profile", new_user_path %>