<% if @errors %>
    <% @errors.each do |error| %>
    <% error %>
    <% end %>
<% end %>


<%= form_for @user do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br><br>
    <%= f.label :bio %>
    <%= f.text_area :bio %>
    <br><br>
    <%= f.label :picture_url %>
    <%= f.url_field :picture %>
    <br><br>
    <%= f.submit %>
<% end %>